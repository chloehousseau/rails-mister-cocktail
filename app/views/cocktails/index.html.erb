

<div>
    <h1>My cocktails</h1>
    <%= link_to "Add a cocktail", new_cocktail_path %>
</div>
<div class="container">
  <div class="row">
    <% @cocktails.each do |cocktail| %>
      <div>
        <div>
            <h2><%= cocktail.name %></h2>
          </div>
          <%= link_to "", cocktail_path(cocktail) %>
        </div>
    <% end %>
  </div>
</div>

